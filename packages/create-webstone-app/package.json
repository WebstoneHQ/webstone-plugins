{
  "name": "create-webstone-app",
  "version": "0.2.1",
  "description": "Start your next web application with Webstone and configure it as you go.",
  "author": "Mike Nikles, @mikenikles",
  "bin": "./bin",
  "scripts": {
    "build": "node ./scripts/esbuild.js build",
    "clean": "rm -fr ./dist",
    "dev": "node ./scripts/esbuild.js dev",
    "prepublishOnly": "pnpm build",
    "prepare": "pnpm build",
    "test": "pnpm test:unit",
    "test:unit": "c8 --all --include=src --reporter=html pnpm test:unit:only",
    "test:unit:only": "uvu -r tsm tests"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "boilerplate",
    "starterkit",
    "web app",
    "graphql"
  ],
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.14.5",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "prompts": "^2.4.2"
  }
}
