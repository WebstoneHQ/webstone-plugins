import type { RequestHandler } from '@sveltejs/kit';

// DELETE {{apiPath}}/:uid
export const DELETE: RequestHandler = async (event) => {
	return new Response(`${event.request.method.toUpperCase()} ${event.url.pathname} => Ok.`, {
		status: 200
	});
};

