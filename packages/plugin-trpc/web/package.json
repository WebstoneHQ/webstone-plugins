{
	"name": "webstone-plugin-trpc-web",
	"version": "0.1.0",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package --input src/lib/plugin && cp package.json ./dist/",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepublishOnly": "pnpm remove webstone-plugin-trpc-cli @webstone/cli"
	},
	"devDependencies": {
		"@playwright/test": "^1.37.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.5",
		"@sveltejs/package": "^2.2.1",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@webstone/cli": "workspace:^0.13.0",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^3.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.9",
		"webstone-plugin-trpc-cli": "workspace:^0.2.0"
	},
	"type": "module",
	"private": true,
	"dependencies": {
		"@trpc/client": "^10.37.1",
		"@trpc/server": "^10.37.1",
		"zod": "^3.21.4"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./index.d.ts",
			"svelte": "./index.js",
			"default": "./index.js"
		}
	},
	"files": [
		"dist"
	]
}
