{
	"name": "webstone-plugin-trpc-web",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package --input src/lib/plugin && cp package.json ./dist/",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepublishOnly": "pnpm remove webstone-plugin-trpc-cli @webstone/cli"
	},
	"devDependencies": {
		"@playwright/test": "^1.29.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@sveltejs/package": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"@webstone/cli": "workspace:^0.11.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"webstone-plugin-trpc-cli": "workspace:^0.0.1"
	},
	"type": "module",
	"private": true,
	"dependencies": {
		"@trpc/client": "^10.7.0",
		"@trpc/server": "^10.7.0",
		"zod": "^3.20.2"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./index.d.ts",
			"svelte": "./index.js",
			"default": "./index.js"
		}
	},
	"files": [
		"dist"
	]
}
