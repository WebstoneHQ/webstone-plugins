{
  "name": "@webstone/core",
  "version": "0.0.21",
  "main": "./dist/index.js",
  "types": "./dist/types/",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/types/"
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm compile",
    "clean": "rm -rf ./dist",
    "compile": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --clean --watch",
    "test": "pnpm test:unit",
    "test:unit": "c8 --all --include=src --reporter=html pnpm test:unit:only",
    "test:unit:only": "NODE_OPTIONS='--loader tsx' uvu tests"
  },
  "author": "Mike Nikles, @mikenikles",
  "license": "MIT",
  "dependencies": {
    "@webstone/cli": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^6.2.3"
  }
}
